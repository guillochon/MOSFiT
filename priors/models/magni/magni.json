{
    "ebv":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "inputs":[
            "transient"
        ],
        "latex":"E({\\rm B} - {\\rm V})"
    },
    "fnickel":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"f_{\\rm Ni}"
    },
    "nhhost":{
        "kind":"parameter",
        "value":1.0e18,
        "class":"parameter",
        "latex":"n_{\\rm H,host}",
        "derived_keys":"avhost"
    },
    "texplosion":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "latex":"t_{\\rm exp}\\,{\\rm (days)}",
        "requests":{
            "min_times": "transient"
        }
    },
    "temperature":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter",
        "latex":"T_{\\min}\\,{\\rm (K)}"
    },
    "Pspin":{
        "kind":"parameter",
        "value":2.0,
        "class":"parameter",
        "latex":"P_{\\rm spin}\\,{\\rm (ms)}"
    },
    "Bfield":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"B"
    },
    "Mns":{
        "kind":"parameter",
        "value":2.0,
        "class":"parameter",
        "latex":"M_{\\rm NS}\\,(M_\\odot)"
    },
    "thetaPB":{
        "kind":"parameter",
        "value":0.785398163,
        "class":"parameter",
        "latex":"\\theta_{\\rm PB}\\,({\\rm radians})"
    },
    "kappa":{
        "kind":"parameter",
        "value":0.2,
        "class":"parameter",
        "latex":"\\kappa\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "kappagamma":{
        "kind":"parameter",
        "value":10.0,
        "class":"parameter",
        "latex":"\\kappa_\\gamma\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "mejecta":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_{\\rm ej}\\,(M_\\odot)"
    },
    "vejecta":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter",
        "latex":"v_{\\rm ej}\\,({\\rm km\\,s}^{-1})"
    },
    "nickelcobalt":{
        "kind":"engine",
        "inputs":[
            "densetimes",
            "mejecta",
            "fnickel",
            "texplosion"
        ],
        "replacements":{
            "luminosities": "nico_luminosities"
        }
    },
    "magnetar":{
        "kind":"engine",
        "inputs":[
            "transient",
            "mejecta",
            "Pspin",
            "Bfield",
            "Mns",
            "thetaPB",
            "texplosion"
        ],
        "replacements":{
            "luminosities": "mag_luminosities"
        }
    },
    "total_luminosity":{
        "kind":"utility",
        "operator":"+",
        "operands":[
            "mag_luminosities",
            "nico_luminosities"
        ],
        "inputs":[
            "nickelcobalt",
            "magnetar"
        ],
        "result":"luminosities",
        "wants_dense":true,
        "class":"operator"
    },
    "diffusion":{
        "kind":"transform",
        "inputs":[
            "total_luminosity",
            "kappa",
            "kappagamma",
            "mejecta",
            "texplosion",
            "vejecta"
        ]
    },
    "densecore":{
        "kind":"photosphere",
        "inputs":[
            "texplosion",
            "diffusion"
        ]
    },
    "blackbody":{
        "kind":"sed",
        "inputs":[
            "texplosion",
            "redshift",
            "densecore"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        }
    },
    "losextinction":{
        "kind":"sed",
        "inputs":[
            "blackbody",
            "nhhost",
            "rvhost",
            "ebv"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        }
    },
    "photometry":{
        "kind":"observable",
        "inputs":[
            "losextinction",
            "lumdist"
        ]
    }
}
