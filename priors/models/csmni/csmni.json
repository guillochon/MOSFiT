{
    "ebv":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "inputs":[
            "transient"
        ],
        "latex":"E({\\rm B} - {\\rm V})"
    },
    "redshift":{
        "kind":"parameter",
        "value":0,
        "class":"parameter",
        "latex":"z"
    },
    "lumdist":{
        "kind":"parameter",
        "value":1.0e-5,
        "class":"parameter",
        "latex":"d"
    },
    "fnickel":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"f_{\\rm Ni}"
    },
    "nhhost":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "latex":"n_{\\rm H,host}",
        "derived_keys":"avhost"
    },
    "texplosion":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "latex":"t_{\\rm exp}\\,{\\rm (days)}",
        "requests":{
            "min_times": "transient"
        }
    },
    "n":{
        "kind":"parameter",
        "value":12.0,
        "class":"parameter",
        "latex":"n"
    },
    "s":{
        "kind":"parameter",
        "value":2.0,
        "class":"parameter",
        "latex":"s"
    },
    "delta":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "latex":"delta"
    },
    "r0":{
        "kind":"parameter",
        "value":60.0,
        "class":"parameter",
        "latex":"R_{0}"
    },
    "mcsm":{
        "kind":"parameter",
        "value":5.01,
        "class":"parameter",
        "latex":"M_{\\rm CSM}"
    },
    "kappa":{
        "kind":"parameter",
        "value":0.34,
        "class":"parameter",
        "latex":"\\kappa\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "kappagamma":{
        "kind":"parameter",
        "value":1000.0,
        "class":"parameter",
        "latex":"\\kappa_\\gamma\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "rho":{
        "kind":"parameter",
        "value":1.0e-12,
        "class":"parameter",
        "latex":"\\rho"
    },
    "mejecta":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_{\\rm ej}\\,(M_\\odot)"
    },
    "kinetic_energy":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"v_{\\rm ej}\\,({\\rm km\\,s}^{-1})"
    },
    "homologous_expansion":{
        "kind":"energetic",
        "inputs":[
            "kinetic_energy",
            "mejecta"
        ]
    },
    "efficiency":{
        "kind":"parameter",
        "value":0.5,
        "class":"parameter",
        "latex":"\\epsilon"
    },
    "temperature":{
        "kind":"parameter",
        "value":100.0,
        "class":"parameter",
        "latex":"T_{\\min}\\,{\\rm (K)}"
    },

    "nickelcobalt":{
        "kind":"engine",
        "inputs":[
            "densetimes",
            "mejecta",
            "fnickel",
            "texplosion"
        ],
        "replacements":{
            "luminosities": "nico_luminosities"
        }
    },
    "csm":{
        "kind":"engine",
        "inputs":[
            "csmconstraints",
            "transient",
            "n",
            "s",
            "delta",
            "r0",
            "mejecta",
            "mcsm",
            "rho",
            "homologous_expansion",
            "texplosion",
            "kappa",
            "efficiency",
            "densetimes"
        ],
        "replacements":{
            "luminosities": "csm_luminosities"
        }
    },
    "mdiffusion":{
        "kind":"utility",
        "operator":"+",
        "operands":[
            "mejecta",
            "mcsmth"
        ],
        "inputs":[
            "csm",
            "mejecta"
        ],
        "result":"mdiffusion",
        "wants_dense":false,
        "class":"operator"
    },
    "diffusion":{
        "kind":"transform",
        "inputs":[
            "nickelcobalt",
            "mdiffusion",
            "kappa",
            "kappagamma",
            "mejecta",
            "texplosion",
            "homologous_expansion"
        ],
        "replacements":{
            "luminosities": "nico_luminosities",
            "mejecta": "mdiffusion"
        }
    },
    "diffusion_csm":{
        "kind":"transform",
        "inputs":[
            "csm",
            "kappa",
            "r0",
            "s",
            "rho",
            "mejecta",
            "mcsm",
            "texplosion",
            "homologous_expansion"
        ],
        "replacements":{
            "luminosities": "csm_luminosities"
        }
    },
    "total_luminosity":{
        "kind":"utility",
        "operator":"+",
        "operands":[
            "csm_luminosities",
            "nico_luminosities"
        ],
        "inputs":[
            "diffusion",
            "diffusion_csm"
        ],
        "result":"luminosities",
        "wants_dense":true,
        "class":"operator"
    },
    "temperature_floor":{
        "kind":"photosphere",
        "inputs":[
            "texplosion",
            "total_luminosity",
            "temperature"
        ],
        "replacements":{
            "luminosities": "luminosities"
        }
    },
    "blackbody":{
        "kind":"sed",
        "inputs":[
            "texplosion",
            "redshift",
            "temperature_floor"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        }
    },
    "csmconstraints":{
        "kind":"constraint",
        "inputs":[
            "n",
            "s",
            "delta",
            "r0",
            "mejecta",
            "mcsm",
            "rho",
            "homologous_expansion",
            "kappa"
        ]
    },
    "losextinction":{
        "kind":"sed",
        "inputs":[
            "blackbody",
            "nhhost",
            "rvhost",
            "ebv"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        }
    },
    "photometry":{
        "kind":"observable",
        "inputs":[
            "losextinction",
            "lumdist"
        ]
    }
}
