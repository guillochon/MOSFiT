{
    "ebv":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "inputs":[
            "transient"
        ],
        "latex":"E({\\rm B} - {\\rm V})"
    },
    "nhhost":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "latex":"n_{\\rm H,host}",
        "derived_keys":"avhost"
    },
    "texplosion":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "latex":"t_{\\rm exp}\\,{\\rm (days)}"
    },
    "n":{
        "kind":"parameter",
        "value":12.0,
        "class":"parameter",
        "latex":"n"
    },
    "s":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "latex":"s"
    },
    "delta":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"delta"
    },
    "r0":{
        "kind":"parameter",
        "value":2.0,
        "class":"parameter",
        "latex":"R_{0}"
    },
    "mcsm":{
        "kind":"parameter",
        "value":5.01,
        "class":"parameter",
        "latex":"M_{\\rm CSM}"
    },
    "kappa":{
        "kind":"parameter",
        "value":0.33,
        "class":"parameter",
        "latex":"\\kappa\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "kappagamma":{
        "kind":"parameter",
        "value":10.0,
        "class":"parameter",
        "latex":"\\kappa_\\gamma\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
   "rho":{
        "kind":"parameter",
        "value":1.0e-12,
        "class":"parameter",
        "latex":"\\rho"
    },
    "mejecta":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_{\\rm ej}\\,(M_\\odot)"
    },
    "vejecta":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter",
        "latex":"v_{\\rm ej}\\,({\\rm km\\,s}^{-1})"
    },
    "temperature":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter",
        "latex":"T_{\\min}\\,{\\rm (K)}"
    },
    "csm":{
        "kind":"engine",
        "inputs":[
            "transient",
            "n",
            "s",
            "delta",
            "r0",
            "mejecta",
            "mcsm",
            "rho",
            "vejecta",
            "texplosion",
            "kappa",
            "densetimes"
        ]
    },
    "diffusion_csm":{
        "kind":"transform",
        "inputs":[
            "csm",
            "kappa",
            "r0",
            "s",
            "rho",
            "mejecta",
            "mcsm",
            "texplosion",
            "vejecta"
        ]
    },
    "temperature_floor":{
        "kind":"photosphere",
        "inputs":[
            "texplosion",
            "diffusion_csm",
            "temperature"
        ]
    },
    "blackbody":{
        "kind":"sed",
        "inputs":[
            "texplosion",
            "redshift",
            "temperature_floor",
            "temperaturephot",
            "radiusphot"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        }
    },
    "losextinction":{
        "kind":"sed",
        "inputs":[
            "blackbody",
            "nhhost",
            "ebv"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        }
    },
    "photometry":{
        "kind":"observable",
        "inputs":[
            "losextinction",
            "lumdist"
        ]
    }
}
