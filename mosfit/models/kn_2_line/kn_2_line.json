{
    "ebv":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "inputs":[
            "transient"
        ],
        "latex":"E({\\rm B} - {\\rm V})"
    },
    "frad":{
        "kind":"parameter",
        "value":0.999,
        "class":"parameter",
        "latex":"f_{\\rm Ni}"
    },
    "nhhost":{
        "kind":"parameter",
        "value":1.0e14,
        "class":"parameter",
        "latex":"n_{\\rm H,host}",
        "derived_keys":"avhost"
    },
    "redshift":{
        "kind":"parameter",
        "value":0.009727,
        "class":"parameter",
        "latex":"z"
    },
    "lumdist":{
        "kind":"parameter",
        "value":39.5,
        "class":"parameter",
        "latex":"d"
    },
    "texplosion":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "latex":"t_{\\rm exp}\\,{\\rm (days)}",
        "requests":{
            "min_times": "transient"
        }
    },
    "temperature_red":{
        "kind":"parameter",
        "value":100,
        "class":"parameter",
        "latex":"T_{\\minr}\\,{\\rm (K)}"
    },
    "temperature_blue":{
        "kind":"parameter",
        "value":100,
        "class":"parameter",
        "latex":"T_{\\minb}\\,{\\rm (K)}"
    },
    "kappa_red":{
        "kind":"parameter",
        "value":10.0,
        "class":"parameter",
        "latex":"\\kappa\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "kappa_blue":{
        "kind":"parameter",
        "value":0.5,
        "class":"parameter",
        "latex":"\\kappa\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "kappagamma":{
        "kind":"parameter",
        "value":100.0,
        "class":"parameter",
        "latex":"\\kappa_\\gamma\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "mejecta_blue":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_{\\rm ejb}\\,(M_\\odot)"
    },
    "mejecta_red":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_{\\rm ejr}\\,(M_\\odot)"
    },
    "vejecta_blue":{
        "kind":"parameter",
        "value":1.0e5,
        "class":"parameter",
        "latex":"v_{\\rm ejb}\\,({\\rm km\\,s}^{-1})"
    },
    "vejecta_red":{
        "kind":"parameter",
        "value":1.0e5,
        "class":"parameter",
        "latex":"v_{\\rm ejr}\\,({\\rm km\\,s}^{-1})"
    },
    "line_wavelength":{
        "kind":"parameter",
        "value":16000,
        "class":"parameter"
    },
    "line_width":{
        "kind":"parameter",
        "value":1000.0,
        "class":"parameter"
    },
    "line_time":{
        "kind":"parameter",
        "value":8.0,
        "class":"parameter"
    },
    "line_duration":{
        "kind":"parameter",
        "value":25.0,
        "class":"parameter"
    },
    "line_amplitude":{
        "kind":"parameter",
        "value":-0.3,
        "class":"parameter"
    },
    "rprocess":{
        "kind":"engine",
        "inputs":[
            "densetimes",
            "mejecta_blue",
            "frad",
            "texplosion",
            "vejecta_blue"
        ],
        "replacements":{
            "mejecta":"mejecta_blue",
            "luminosities":"luminosities_blue",
            "vejecta":"vejecta_blue"
        }
    },
    "diffusion":{
        "kind":"transform",
        "inputs":[
            "rprocess",
            "kappagamma",
            "kappa_blue",
            "mejecta_blue",
            "texplosion",
            "vejecta_blue"
        ],
        "replacements":{
            "luminosities":"luminosities_blue",
            "mejecta":"mejecta_blue",
            "vejecta":"vejecta_blue",
            "kappa":"kappa_blue",
            "kappa_bluegamma":"kappagamma"
        }
    },
    "temperature_floor":{
        "kind":"photosphere",
        "inputs":[
            "texplosion",
            "temperature_blue",
            "diffusion"
        ],
        "replacements":{
            "luminosities": "luminosities_blue",
            "vejecta":"vejecta_blue",
            "mejecta":"mejecta_blue",
            "kappa":"kappa_blue",
            "temperature":"temperature_blue",
            "temperature_bluephot":"temperaturephot"
        }
    },
    "blackbody":{
        "kind":"sed",
        "inputs":[
            "texplosion",
            "redshift",
            "temperature_floor"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "luminosities":"luminosities_blue"
        }
    },
    "rprocess2":{
        "kind":"engine",
        "inputs":[
            "densetimes",
            "mejecta_red",
            "frad",
            "texplosion",
            "vejecta_red"
        ],
        "replacements":{
            "mejecta":"mejecta_red",
            "luminosities":"luminosities_red",
            "vejecta":"vejecta_red"
        }
    },
    "diffusion2":{
        "kind":"transform",
        "inputs":[
            "rprocess2",
            "kappa_red",
            "kappagamma",
            "mejecta_red",
            "texplosion",
            "vejecta_red"
        ],
        "replacements":{
            "luminosities":"luminosities_red",
            "mejecta":"mejecta_red",
            "vejecta":"vejecta_red",
            "kappa":"kappa_red",
            "kappa_redgamma":"kappagamma"
        }
    },
    "temperature_floor2":{
        "kind":"photosphere",
        "inputs":[
            "texplosion",
            "temperature_red",
            "diffusion2"
        ],
        "replacements":{
            "luminosities": "luminosities_red",
            "vejecta":"vejecta_red",
            "mejecta":"mejecta_red",
            "kappa":"kappa_red",
            "temperature":"temperature_red",
            "temperature_redphot":"temperaturephot"
        }
    },
    "blackbody2":{
        "kind":"sed",
        "inputs":[
            "texplosion",
            "redshift",
            "temperature_floor2",
            "blackbody"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "luminosities":"luminosities_red"
        }
    },
    "line":{
        "kind":"sed",
        "inputs":[
            "blackbody2",
            "redshift",
            "line_wavelength",
            "line_width",
            "line_time",
            "line_duration",
            "line_amplitude"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "luminosities":"luminosities_red"
        }
    },
    "photometry":{
        "kind":"observable",
        "inputs":[
            "line",
            "lumdist"
        ],
        "replacements":{
            "luminosities":"luminosities_red"
        }
    }
}
