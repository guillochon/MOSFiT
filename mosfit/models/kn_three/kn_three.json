{
    "ebv":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "inputs":[
            "transient"
        ],
        "latex":"E({\\rm B} - {\\rm V})"
    },
    "frad":{
        "kind":"parameter",
        "value":0.999,
        "class":"parameter",
        "latex":"f_{\\rm Ni}"
    },
    "nhhost":{
        "kind":"parameter",
        "value":1.0e14,
        "class":"parameter",
        "latex":"n_{\\rm H,host}",
        "derived_keys":"avhost"
    },
    "redshift":{
        "kind":"parameter",
        "value":0.009727,
        "class":"parameter",
        "latex":"z"
    },
    "lumdist":{
        "kind":"parameter",
        "value":43.36,
        "class":"parameter",
        "latex":"d"
    },
    "texplosion":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "latex":"t_{\\rm exp}\\,{\\rm (days)}",
        "requests":{
            "min_times": "transient"
        }
    },
    "temperature":{
        "kind":"parameter",
        "value":100,
        "class":"parameter",
        "latex":"T_{\\min}\\,{\\rm (K)}"
    },
    "kappa_red":{
        "kind":"parameter",
        "value":10.0,
        "class":"parameter",
        "latex":"\\kappa\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "kappa_blue":{
        "kind":"parameter",
        "value":0.5,
        "class":"parameter",
        "latex":"\\kappa\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "kappa_purple":{
        "kind":"parameter",
        "value":3.0,
        "class":"parameter",
        "latex":"\\kappa\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "kappagamma":{
        "kind":"parameter",
        "value":100.0,
        "class":"parameter",
        "latex":"\\kappa_\\gamma\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "mejecta_blue":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_{\\rm ejb}\\,(M_\\odot)"
    },
    "mejecta_red":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_{\\rm ejr}\\,(M_\\odot)"
    },
    "mejecta_purple":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_{\\rm ejp}\\,(M_\\odot)"
    },
    "vejecta_blue":{
        "kind":"parameter",
        "value":1.0e5,
        "class":"parameter",
        "latex":"v_{\\rm ejb}\\,({\\rm km\\,s}^{-1})"
    },
    "vejecta_red":{
        "kind":"parameter",
        "value":1.0e5,
        "class":"parameter",
        "latex":"v_{\\rm ejr}\\,({\\rm km\\,s}^{-1})"
    },
    "vejecta_purple":{
        "kind":"parameter",
        "value":1.0e5,
        "class":"parameter",
        "latex":"v_{\\rm ejp}\\,({\\rm km\\,s}^{-1})"
    },
    "rprocess":{
        "kind":"engine",
        "inputs":[
            "densetimes",
            "mejecta_blue",
            "frad",
            "texplosion",
            "vejecta_blue"
        ],
        "replacements":{
            "mejecta":"mejecta_blue",
            "luminosities":"luminosities_blue",
            "vejecta":"vejecta_blue"
        }
    },
    "diffusion":{
        "kind":"transform",
        "inputs":[
            "rprocess",
            "kappagamma",
            "kappa_blue",
            "mejecta_blue",
            "texplosion",
            "vejecta_blue"
        ],
        "replacements":{
            "luminosities":"luminosities_blue",
            "mejecta":"mejecta_blue",
            "vejecta":"vejecta_blue",
            "kappa":"kappa_blue",
            "kappa_bluegamma":"kappagamma"
        }
    },
    "rprocess2":{
        "kind":"engine",
        "inputs":[
            "densetimes",
            "mejecta_red",
            "frad",
            "texplosion",
            "vejecta_red"
        ],
        "replacements":{
            "mejecta":"mejecta_red",
            "luminosities":"luminosities_red",
            "vejecta":"vejecta_red"
        }
    },
    "diffusion2":{
        "kind":"transform",
        "inputs":[
            "rprocess2",
            "kappa_red",
            "kappagamma",
            "mejecta_red",
            "texplosion",
            "vejecta_red"
        ],
        "replacements":{
            "luminosities":"luminosities_red",
            "mejecta":"mejecta_red",
            "vejecta":"vejecta_red",
            "kappa":"kappa_red",
            "kappa_redgamma":"kappagamma"
        }
    },
    "rprocess3":{
        "kind":"engine",
        "inputs":[
            "densetimes",
            "mejecta_purple",
            "frad",
            "texplosion",
            "vejecta_purple"
        ],
        "replacements":{
            "mejecta":"mejecta_purple",
            "luminosities":"luminosities_purple",
            "vejecta":"vejecta_purple"
        }
    },
    "diffusion3":{
        "kind":"transform",
        "inputs":[
            "rprocess3",
            "kappa_purple",
            "kappagamma",
            "mejecta_purple",
            "texplosion",
            "vejecta_purple"
        ],
        "replacements":{
            "luminosities":"luminosities_purple",
            "mejecta":"mejecta_purple",
            "vejecta":"vejecta_purple",
            "kappa":"kappa_purple",
            "kappa_purplegamma":"kappagamma"
        }
    },
    "total_luminosity":{
        "kind":"utility",
        "operator":"+",
        "operands":[
            "luminosities_red",
            "luminosities_blue",
            "luminosities_purple"
        ],
        "inputs":[
            "rprocess",
            "rprocess2",
            "rprocess3"
        ],
        "result":"luminosities",
        "wants_dense":true,
        "class":"operator"
    },
    "temperature_floor":{
        "kind":"photosphere",
        "inputs":[
            "texplosion",
            "total_luminosity",
            "temperature",
            "diffusion2",
            "diffusion",
            "diffusion3"
        ],
        "replacements":{
            "luminosities": "luminosities",
            "vejecta":"vejecta_red",
            "mejecta":"mejecta_red",
            "kappa":"kappa_red"
        }
    },
    "blackbody":{
        "kind":"sed",
        "inputs":[
            "texplosion",
            "redshift",
            "temperature_floor"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        }
    },
    "losextinction":{
        "kind":"sed",
        "inputs":[
            "blackbody",
            "nhhost",
            "rvhost",
            "ebv"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        }
    },
    "photometry":{
        "kind":"observable",
        "inputs":[
            "losextinction",
            "lumdist"
        ]
    }
}
