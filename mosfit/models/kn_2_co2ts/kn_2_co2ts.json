{
    "ebv":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "inputs":[
            "transient"
        ],
        "latex":"E({\\rm B} - {\\rm V})"
    },
    "frad":{
        "kind":"parameter",
        "value":0.999,
        "class":"parameter",
        "latex":"f_{\\rm Ni}"
    },
    "nhhost":{
        "kind":"parameter",
        "value":1.0e14,
        "class":"parameter",
        "latex":"n_{\\rm H,host}",
        "derived_keys":"avhost"
    },
    "redshift":{
        "kind":"parameter",
        "value":0.009727,
        "class":"parameter",
        "latex":"z"
    },
    "lumdist":{
        "kind":"parameter",
        "value":39.5,
        "class":"parameter",
        "latex":"d"
    },
    "texplosion":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "latex":"t_{\\rm exp}\\,{\\rm (days)}",
        "requests":{
            "min_times": "transient"
        }
    },
    "temperature_red":{
        "kind":"parameter",
        "value":100,
        "class":"parameter",
        "latex":"T_{\\minr}\\,{\\rm (K)}"
    },
    "temperature_blue":{
        "kind":"parameter",
        "value":100,
        "class":"parameter",
        "latex":"T_{\\minb}\\,{\\rm (K)}"
    },
    "kappa_red":{
        "kind":"parameter",
        "value":10.0,
        "class":"parameter",
        "latex":"\\kappa\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "kappa_blue":{
        "kind":"parameter",
        "value":0.5,
        "class":"parameter",
        "latex":"\\kappa\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "kappagamma":{
        "kind":"parameter",
        "value":100.0,
        "class":"parameter",
        "latex":"\\kappa_\\gamma\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "mejecta_blue":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_{\\rm ejb}\\,(M_\\odot)"
    },
    "mejecta_red":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_{\\rm ejr}\\,(M_\\odot)"
    },
    "vejecta_blue":{
        "kind":"parameter",
        "value":1.0e5,
        "class":"parameter",
        "latex":"v_{\\rm ejb}\\,({\\rm km\\,s}^{-1})"
    },
    "vejecta_red":{
        "kind":"parameter",
        "value":1.0e5,
        "class":"parameter",
        "latex":"v_{\\rm ejr}\\,({\\rm km\\,s}^{-1})"
    },
    "cutoff_wavelength_blue":{
        "kind":"parameter",
        "value":3000.0,
        "class":"parameter",
        "latex":"\\lambda_{\\rm cutoffblue}"
    },
    "cutoff_wavelength_red":{
        "kind":"parameter",
        "value":10000.0,
        "class":"parameter",
        "latex":"\\lambda_{\\rm cutoffred}"
    },
    "cutoff_wavelength_red_stop":{
        "kind":"parameter",
        "value":10000.0,
        "class":"parameter",
        "latex":"\\lambda_{\\rm cutoffred}"
    },
    "cutoff_wavelength_blue_stop":{
        "kind":"parameter",
        "value":10000.0,
        "class":"parameter",
        "latex":"\\lambda_{\\rm cutoffred}"
    },
    "cutoff_wavelength_slope_red":{
        "kind":"parameter",
        "value":0.01,
        "class":"parameter",
        "latex":"slopered"
    },
    "cutoff_wavelength_slope_blue":{
        "kind":"parameter",
        "value":0.01,
        "class":"parameter",
        "latex":"slopeblue"
    },
    "rprocess":{
        "kind":"engine",
        "inputs":[
            "densetimes",
            "mejecta_blue",
            "frad",
            "texplosion",
            "vejecta_blue"
        ],
        "replacements":{
            "mejecta":"mejecta_blue",
            "luminosities":"luminosities_blue",
            "vejecta":"vejecta_blue"
        }
    },
    "diffusion":{
        "kind":"transform",
        "inputs":[
            "rprocess",
            "kappagamma",
            "kappa_blue",
            "mejecta_blue",
            "texplosion",
            "vejecta_blue"
        ],
        "replacements":{
            "luminosities":"luminosities_blue",
            "mejecta":"mejecta_blue",
            "vejecta":"vejecta_blue",
            "kappa":"kappa_blue",
            "kappa_bluegamma":"kappagamma"
        }
    },
    "temperature_floor":{
        "kind":"photosphere",
        "inputs":[
            "texplosion",
            "temperature_blue",
            "diffusion"
        ],
        "replacements":{
            "luminosities": "luminosities_blue",
            "vejecta":"vejecta_blue",
            "mejecta":"mejecta_blue",
            "kappa":"kappa_blue",
            "temperature":"temperature_blue",
            "temperature_bluephot":"temperaturephot"
        }
    },
    "blackbody_cutoff_time_stop":{
        "kind":"sed",
        "inputs":[
            "texplosion",
            "redshift",
            "temperature_floor",
            "cutoff_wavelength_blue",
            "cutoff_wavelength_slope_blue",
            "cutoff_wavelength_blue_stop"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "luminosities":"luminosities_blue",
            "cutoff_wavelength":"cutoff_wavelength_blue",
            "cutoff_wavelength_blue_slope":"cutoff_wavelength_slope_blue"
        }
    },
    "rprocess2":{
        "kind":"engine",
        "inputs":[
            "densetimes",
            "mejecta_red",
            "frad",
            "texplosion",
            "vejecta_red"
        ],
        "replacements":{
            "mejecta":"mejecta_red",
            "luminosities":"luminosities_red",
            "vejecta":"vejecta_red"
        }
    },
    "diffusion2":{
        "kind":"transform",
        "inputs":[
            "rprocess2",
            "kappa_red",
            "kappagamma",
            "mejecta_red",
            "texplosion",
            "vejecta_red"
        ],
        "replacements":{
            "luminosities":"luminosities_red",
            "mejecta":"mejecta_red",
            "vejecta":"vejecta_red",
            "kappa":"kappa_red",
            "kappa_redgamma":"kappagamma"
        }
    },
    "temperature_floor2":{
        "kind":"photosphere",
        "inputs":[
            "texplosion",
            "temperature_red",
            "diffusion2"
        ],
        "replacements":{
            "luminosities": "luminosities_red",
            "vejecta":"vejecta_red",
            "mejecta":"mejecta_red",
            "kappa":"kappa_red",
            "temperature":"temperature_red",
            "temperature_redphot":"temperaturephot"
        }
    },
    "blackbody_cutoff_time_stop2":{
        "kind":"sed",
        "inputs":[
            "texplosion",
            "redshift",
            "temperature_floor2",
            "cutoff_wavelength_red",
            "blackbody_cutoff_time_stop",
            "cutoff_wavelength_slope_red",
            "cutoff_wavelength_red_stop"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "luminosities":"luminosities_red",
            "cutoff_wavelength":"cutoff_wavelength_red",
            "cutoff_wavelength_red_slope":"cutoff_wavelength_slope_red"
        }
    },
    "photometry":{
        "kind":"observable",
        "inputs":[
            "blackbody_cutoff_time_stop2",
            "lumdist"
        ],
        "replacements":{
            "luminosities":"luminosities_red"
        }
    }
}
