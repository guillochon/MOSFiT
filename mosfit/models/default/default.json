{
    "transient":{
        "kind":"data",
        "keys":{
            "photometry":{
                "time":["required", "numeric"],
                "magnitude":["required", "numeric"],
                "e_magnitude":["required", "numeric"],
                "band":"required",
                "instrument":"optional",
                "system":"optional",
                "upperlimit":"exclude",
                "includeshost":"exclude",
                "host":"exclude",
                "kcorrected":"exclude",
                "scorrected":"exclude"
            },
            "ebv":{
                "value":"required"
            },
            "lumdist":{
                "value":"required"
            },
            "redshift":{
                "value":"required"
            }
        }
    },
    "fnickel":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"f_{\\rm Ni}"
    },
    "nhhost":{
        "kind":"parameter",
        "value":1.0e18,
        "class":"parameter",
        "latex":"n_{\\rm H,host}"
    },
    "texplosion":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "latex":"t_{\\rm exp}\\,{\\rm (days)}"
    },
    "temperature":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter",
        "latex":"T_{\\min}\\,{\\rm (K)}"
    },
    "kappa":{
        "kind":"parameter",
        "value":0.2,
        "class":"parameter",
        "latex":"\\kappa\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "kappagamma":{
        "kind":"parameter",
        "value":10.0,
        "class":"parameter",
        "latex":"\\kappa_\\gamma\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "mejecta":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_{\\rm ej}\\,(M_\\odot)"
    },
    "vejecta":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter",
        "latex":"v_{\\rm ej}\\,({\\rm km\\,s}^{-1})"
    },
    "variance":{
        "kind":"parameter",
        "class":"parameter",
        "latex":"\\sigma"
    },
    "densetimes":{
        "kind":"array",
        "inputs":[
            "transient",
            "texplosion"
        ]
    },
    "nickelcobalt":{
        "kind":"engine",
        "inputs":[
            "densetimes",
            "mejecta",
            "fnickel",
            "texplosion"
        ]
    },
    "diffusion":{
        "kind":"transform",
        "inputs":[
            "nickelcobalt",
            "kappa",
            "kappagamma",
            "mejecta",
            "texplosion",
            "vejecta"
        ]
    },
    "photosphere":{
        "kind":"sed",
        "inputs":[
            "texplosion",
            "diffusion",
            "temperature"
        ]
    },
    "extinction":{
        "kind":"sed",
        "inputs":[
            "photosphere",
            "nhhost"
        ],
        "requests":[
            ["bandwavelengths", "filters"],
            []
        ]
    },
    "filters":{
        "kind":"observable",
        "inputs":[
            "extinction"
        ],
        "requests":[
            ["band_wave_ranges", "filters"]
        ]
    },
    "likelihood":{
        "kind":"objective",
        "inputs":[
            "transient",
            "variance",
            "filters"
        ]
    },
    "fitlc":{
        "kind":"output",
        "inputs":[
            "likelihood",
            "transient",
            "filters"
        ],
        "class":"lightcurve"
    }
}
